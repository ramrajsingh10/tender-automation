{
  "hosting": [
    {
      "target": "tender-app",
      "public": "frontend/out",
      "predeploy": [
        "npm --prefix frontend install --legacy-peer-deps",
        "npm --prefix frontend run build"
      ],
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "cleanUrls": true,
      "trailingSlash": false
    }
  ]
}

